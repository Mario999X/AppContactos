<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="principalAnidada" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appcontactos.AppAnidadaController">
   <children>
      <VBox fx:id="vBoxFondo" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #33ACFF;">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Button fx:id="btnFlecha" mnemonicParsing="false" onAction="#volverPrincipal" text="Flecha" />
         </children>
      </VBox>
      <VBox fx:id="vBoxCentral" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: FF5733;" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets left="200.0" right="200.0" />
         </StackPane.margin>
         <children>
            <ImageView fx:id="imagenContacto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="25.0" right="25.0" top="30.0" />
               </VBox.margin>
               <image>
                  <Image url="@src/main/resources/Avatar.png" />
               </image>
            </ImageView>
            <VBox fx:id="vBoxInfo" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="nombreContacto" text="Nombre: " />
                  <Label fx:id="correoContacto" text="Correo: " />
                  <Label fx:id="otrosContacto" text="Otros datos: " />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
   </children>
</StackPane>
